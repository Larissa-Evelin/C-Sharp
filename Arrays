using System;

namespace teste
{
    class Program
    {
        static void Main(string[] args)
        {
            string[] a = new string[3];

            a[0] = "Larissa";
            a[1] = "Maranhase";
            a[2] = "Evelin";

            Array.Sort(a); // função para colocar as palavras em ondem alfabetica

            foreach (string nome in a)
            {
                Console.WriteLine(nome);
            }
            Console.Read();
        }
        }
    }
